{
  "name": "restapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www --exec babel-node --presets @babel/preset-env",
    "test": "./node_modules/.bin/jest tests --coverage",
    "autotest": "npm run migrate:reset && npm run migrate && npm run test",
    "migrate": "sequelize db:migrate",
    "migrate:reset": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.18.2",
    "http-errors": "^1.6.3",
    "http-status": "^1.3.2",
    "jest": "^24.9.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "mysql2": "^1.6.5",
    "pug": "^2.0.4",
    "random-string": "^0.2.0",
    "sequelize": "^5.16.0",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.2.2",
    "nodemon": "^1.19.1",
    "regenerator-runtime": "^0.13.3",
    "supertest": "^4.0.2"
  }
